package java111Q1;


public class Class01 {
	public static void main (String args[]) {
		int r ;
		int length;
		int width ;
		int height ;
		double volume;
		int area;
		
		 r = 10;
		 length = 4;
		 width = 5;
		 height = 6;
		 volume = (4*3.14159*10*10*10)/3;
		 area = (4*5)+(5*6)+(4*6)*2;
		 
		 System.out.println(volume);
		 System.out.println(area);
		
		
	}
}
